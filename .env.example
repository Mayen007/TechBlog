# Copy this file to a file named .env in the project root and fill in values.
# Do NOT commit your real .env to version control.

# Admin credentials (ADMIN_PASSWORD should be a hashed password produced
# by `generate_password.py` or another secure password-hashing tool.)
ADMIN_USERNAME=admin
# Example hashed password format (do not use this exact value). Generate your own:
ADMIN_PASSWORD=scrypt:32768:8:1$...$...

# Other environment variables (database URL, secret keys, etc.) can go here.

# Example: FLASK_ENV=development
FLASK_ENV=development
